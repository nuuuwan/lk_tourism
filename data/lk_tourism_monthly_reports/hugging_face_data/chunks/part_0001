    "doc_type": "lk_tourism_monthly_reports",
    "doc_id": "2024-09-01-2024-09-01",
    "num": "2024-09-01",
    "date_str": "2024-09-01",
    "description": "Arrivals report September 2024",
    "url_metadata": "https://www.sltda.gov.lk/en/monthly-tourist-arrivals-reports-2024",
    "lang": "en",
    "url_pdf": "https://www.sltda.gov.lk/storage/common_media/MonthlyTouritsArrivalsReportSeptember2024-10-14.pdf",
